{
  "openapi": "3.0.1",
  "info": {
    "title": "UserService",
    "version": "1.0"
  },
  "paths": {
    "/user/AddUser": {
      "post": {
        "tags": [
          "User"
        ],
        "operationId": "AddUser",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLoginDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UserLoginDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UserLoginDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/user/GetUser/{id}": {
      "get": {
        "tags": [
          "User"
        ],
        "operationId": "GetUsers",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "UserLoginDTO": {
        "type": "object",
        "properties": {
          "userName": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}